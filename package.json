{
  "name": "@abcnews/components-storylab",
  "version": "0.0.1",
  "scripts": {
    "dev": "npm run storybook",
    "build": "npm run prepack",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "prepack": "svelte-kit sync && svelte-package && publint",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "test": "npm run test:unit -- --run",
    "storybook": "export AUNTY_HOST=${AUNTY_HOST:-localhost} && storybook dev -p 6006 --host $AUNTY_HOST --ci",
    "build-storybook": "storybook build"
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    },
    "./mapLibre": {
      "types": "./dist/mapLibre/index.d.ts",
      "default": "./dist/mapLibre/index.js"
    }
  },
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "devDependencies": {
    "@storybook/addon-svelte-csf": "^5.0.10",
    "@storybook/sveltekit": "^10.1.11",
    "@sveltejs/adapter-auto": "^7.0.0",
    "@sveltejs/kit": "^2.49.1",
    "@sveltejs/package": "^2.5.7",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@types/node": "^24",
    "@vitest/browser-playwright": "^4.0.15",
    "maplibre-gl": "^5.16.0",
    "playwright": "^1.57.0",
    "publint": "^0.3.15",
    "sass-embedded": "^1.97.2",
    "storybook": "^10.1.11",
    "svelte": "^5.45.6",
    "svelte-check": "^4.3.4",
    "typescript": "^5.9.3",
    "vite": "^7.2.6",
    "vitest": "^4.0.15",
    "vitest-browser-svelte": "^2.0.1"
  },
  "keywords": [
    "svelte"
  ],
  "license": "MIT",
  "homepage": "https://github.com/abcnews/components-storylab",
  "repository": "github:abcnews/components-storylab"
}
